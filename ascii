#!/usr/bin/env python
from typing import Iterable
import sys

from typing_extensions import Final


def strip_non_ascii(k: Iterable[str]) -> str:
    return ''.join(x for x in k if ord(x) < 128)


def main() -> int:
    if len(sys.argv) > 1:
        lines = sys.argv[1:]
    else:
        lines = sys.stdin.readlines()
    original: Final = ''.join(x.strip() for x in lines)
    stripped: Final = ''.join(strip_non_ascii(x.strip()) for x in lines)
    if len(original) != len(stripped):
        return 1
    return 0


if __name__ == '__main__':
    sys.exit(main())
